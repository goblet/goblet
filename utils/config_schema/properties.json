    "function_name": {
      "type": "string"
    },
    "custom_files": {
      "type": "object",
      "properties": {
        "include": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exclude": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "deploy": {
      "type": "object",
      "properties": {
        "environmentVariables": {
          "type": "object",
